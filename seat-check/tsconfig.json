{
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "bundler",
    "moduleSuffixes": [".web", ".native", ""],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": ["node_modules"]
}
